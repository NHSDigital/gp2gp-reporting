{
  "name": "prm-gp2gp-dashboard",
  "private": true,
  "description": "GP2GP Service Dashboard",
  "version": "1.0.0",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "test": "jest",
    "format": "prettier --write \"**/*.{js,jsx,scss,md}\"",
    "checkFormat": "prettier --check \"**/*.{js,jsx,scss,md}\"",
    "getMetrics": "node scripts/get-s3-data.js --datatype metrics",
    "getMetadata": "node scripts/get-s3-data.js --datatype metadata"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkFormat && npm test && npm run build"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.637.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.19.39",
    "gatsby-plugin-layout": "^1.1.23",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-s3": "^0.3.3",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-source-filesystem": "^2.1.53",
    "gatsby-transformer-json": "^2.2.27",
    "lodash": "^4.17.15",
    "nhsuk-frontend": "^3.0.3",
    "node-sass": "^4.13.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^6.15.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.2.1",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.35",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "moxios": "^0.4.0",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.13.0"
  }
}