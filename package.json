{
  "name": "prm-gp2gp-dashboard",
  "private": true,
  "description": "GP Registrations Data",
  "version": "1.0.0",
  "scripts": {
    "mockServer": "node local-mocks/mockServer.js",
    "develop": "GATSBY_ODS_PORTAL_URL=http://localhost:3000/ORD/2-0-0/organisations concurrently --kill-others \"npm run mockServer\" \"gatsby develop\"",
    "develop:ci": "gatsby develop",
    "build": "GATSBY_ODS_PORTAL_URL=http://localhost:3000/ORD/2-0-0/organisations gatsby build",
    "audit": "better-npm-audit audit",
    "build:ci": "GATSBY_CPU_COUNT=2 NODE_OPTIONS=\"--max-old-space-size=8192\" gatsby build",
    "serve": "concurrently --kill-others \"npm run mockServer\" \"gatsby serve\"",
    "serve:ci": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "test": "jest --silent",
    "e2e": "./node_modules/.bin/cypress run",
    "e2e:install": "./node_modules/.bin/cypress install",
    "e2e:open": "./node_modules/.bin/cypress open",
    "test:e2e": "start-server-and-test 'gatsby develop' http://localhost:8000 e2e",
    "test:e2e:open": "start-server-and-test 'gatsby develop' http://localhost:8000 e2e:open",
    "test:e2e:built": "CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test 'gatsby serve' http://localhost:9000 e2e",
    "format": "prettier --write \"**/*.{js,jsx,json,scss,md,ts,tsx}\"",
    "getPracticeMetrics:ci": "node scripts/getMetricsFromS3.js --datatype practiceMetrics",
    "getPracticeMetrics:stub": "node scripts/getStubData.js --datatype practiceMetrics",
    "getNationalMetrics:ci": "node scripts/getMetricsFromS3.js --datatype nationalMetrics",
    "getNationalMetrics:stub": "node scripts/getStubData.js --datatype nationalMetrics",
    "enableGitHooks": "husky install",
    "pre-commit": "lint-staged && npm test",
    "pre-push": "npm run build && npm run test:e2e:built",
    "preinstall": "npx npm-force-resolutions",
    "getStubs": "npm run getPracticeMetrics:stub && npm run getNationalMetrics:stub && npm run clean"
  },
  "lint-staged": {
    "*.{js,jsx,json,scss,md,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "aws-sdk": "^2.1103.0",
    "axios": "0.26.1",
    "better-npm-audit": "^3.7.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.0",
    "fs-extra": "^10.0.1",
    "gatsby": "~4.9.3",
    "gatsby-plugin-layout": "^3.11.0",
    "gatsby-plugin-react-helmet": "^5.11.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sass": "^5.12.1",
    "gatsby-plugin-ts-checker": "^1.1.0",
    "gatsby-source-filesystem": "^4.14.0",
    "gatsby-transformer-json": "^4.11.0",
    "js-search": "^2.0.0",
    "lodash": "^4.17.21",
    "nhsuk-frontend": "^6.1.0",
    "no-scroll": "^2.1.1",
    "react": "^17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.17.0",
    "react-responsive-modal": "^6.2.0",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.7",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/jest-when": "^2.7.4",
    "@types/moxios": "^0.4.15",
    "@types/no-scroll": "^2.1.0",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.5",
    "axe-core": "^4.4.1",
    "axios-mock-adapter": "^1.20.0",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby": "^2.11.0",
    "concurrently": "^6.5.1",
    "cypress": "^7.7.0",
    "cypress-axe": "^0.14.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-when": "^3.5.1",
    "json-server": "^0.16.3",
    "lint-staged": "^11.2.6",
    "moxios": "^0.4.0",
    "prettier": "^2.3.1",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "async": "^3.2.2",
    "ansi-html": "^0.0.9",
    "netmask": "^2.0.1",
    "mem": "^8.1.1",
    "browserslist": "^4.16.5",
    "ws": "^8.5.0",
    "pac-resolver": "^5.0.0",
    "immer": "^9.0.6",
    "node-fetch": "^2.6.7"
  }
}
