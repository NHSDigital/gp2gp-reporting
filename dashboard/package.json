{
  "name": "gp2gp-reporting",
  "private": true,
  "description": "GP Registrations Data",
  "version": "1.0.0",
  "scripts": {
    "mockServer": "node local-mocks/mockServer.js",
    "develop": "GATSBY_ODS_PORTAL_URL=http://localhost:3000/ORD/2-0-0/organisations concurrently --kill-others \"npm run mockServer\" \"gatsby develop\"",
    "develop:ci": "gatsby develop",
    "build": "GATSBY_ODS_PORTAL_URL=http://localhost:3000/ORD/2-0-0/organisations gatsby build",
    "audit": "better-npm-audit audit",
    "build:ci": "GATSBY_CPU_COUNT=2 NODE_OPTIONS=\"--max-old-space-size=8192\" gatsby build",
    "serve": "concurrently --kill-others \"npm run mockServer\" \"gatsby serve\"",
    "serve:ci": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "deploy:ci": "gatsby-plugin-s3 deploy -y",
    "test": "jest --silent",
    "e2e": "./node_modules/.bin/cypress run",
    "e2e:install": "./node_modules/.bin/cypress install",
    "e2e:open": "./node_modules/.bin/cypress open",
    "test:e2e": "start-server-and-test 'gatsby develop' http://localhost:8000 e2e",
    "gatsby:develop": "gatsby develop",
    "test:e2e:open": "start-server-and-test 'gatsby develop' http://localhost:8000 e2e:open",
    "test:e2e:built": "CYPRESS_BASE_URL=http://localhost:9000 start-server-and-test 'gatsby serve' http://localhost:9000 e2e",
    "format": "prettier --write \"**/*.{js,jsx,json,scss,md,ts,tsx}\"",
    "getPracticeMetrics:ci": "node scripts/getMetricsFromS3.js --datatype practiceMetrics",
    "getPracticeMetrics:stub": "node scripts/getStubData.js --datatype practiceMetrics",
    "getNationalMetrics:ci": "node scripts/getMetricsFromS3.js --datatype nationalMetrics",
    "getNationalMetrics:stub": "node scripts/getStubData.js --datatype nationalMetrics",
    "enableGitHooks": "husky install",
    "pre-commit": "lint-staged && npm test",
    "pre-push": "npm run build && npm run test:e2e:built",
    "getStubs": "npm run getPracticeMetrics:stub && npm run getNationalMetrics:stub && npm run clean"
  },
  "lint-staged": {
    "*.{js,jsx,json,scss,md,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "aws-sdk": "^2.1693.0",
    "axios": "1.13.5",
    "better-npm-audit": "^3.11.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "fs-extra": "^11.3.3",
    "gatsby": "^5.16.0",
    "gatsby-plugin-layout": "^4.16.0",
    "gatsby-plugin-react-helmet": "^6.16.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-plugin-s3": "^0.4.1",
    "gatsby-plugin-sass": "^6.16.0",
    "gatsby-plugin-ts-checker": "^1.1.0",
    "gatsby-source-filesystem": "^5.16.0",
    "gatsby-transformer-json": "^5.16.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-search": "^2.0.1",
    "lodash": "^4.17.23",
    "nhsuk-frontend": "^6.1.0",
    "no-scroll": "^2.1.1",
    "react": "^19.2.4",
    "react-autosuggest": "^10.1.0",
    "react-cookie": "^8.0.1",
    "react-dom": "^19.2.4",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.21.0",
    "react-responsive-modal": "^7.1.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-when": "^3.5.5",
    "@types/lodash": "^4.17.23",
    "@types/moxios": "^0.4.17",
    "@types/no-scroll": "^2.1.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "axe-core": "^4.11.1",
    "axios-mock-adapter": "2.1.0",
    "babel-jest": "30.2.0",
    "babel-preset-gatsby": "^3.16.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.10.0",
    "cypress-axe": "^1.7.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-when": "^3.7.0",
    "json-server": "^0.17.4",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react-test-renderer": "^19.2.4",
    "start-server-and-test": "^2.1.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "got": "11.8.6",
    "loader-utils": "2.0.4",
    "netmask": "2.0.2",
    "degenerator": "3.0.4",
    "pac-resolver": "7.0.1",
    "xml2js": "^0.6.2",
    "form-data": "4.0.5",
    "multer": "2.0.2",
    "tmp": "^0.2.4",
    "postcss": "^8.4.31",
    "cookie": "0.7.1",
    "tough-cookie": "4.1.4",
    "mem": "^9.0.2",
    "qs": "6.14.1",
    "webpack": "^5.104.0"
  }
}
